{
	"name": "magnus",
	"displayName": "Magnus",
	"description": "Magnus remote server query provider.",
	"publisher": "Triumph Tech",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.41.0",
		"azdata": "*"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onConnect:magnus",
		"onCommand:magnus.helloWorld",
		"onCommand:magnus.showCurrentConnection"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "magnus.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "magnus.showCurrentConnection",
				"title": "Show Current Connection"
			}
		],
		"connectionProvider": {
			"providerId": "magnus",
			"displayName": "Magnus",
			"connectionOptions": [
			  {
				"specialValueType": "serverName",
				"isIdentity": true,
				"name": "server",
				"displayName": "Server",
				"groupName": "Source",
				"valueType": "string",
				"defaultValue": null,
				"objectType": null,
				"categoryValues": null,
				"isRequired": true,
				"isArray": false
			  },
			  {
				"specialValueType": "databaseName",
				"isIdentity": true,
				"name": "database",
				"displayName": "not used",
				"groupName": "Source",
				"valueType": "string",
				"defaultValue": null,
				"objectType": null,
				"categoryValues": null,
				"isRequired": false,
				"isArray": false
			  },
			  {
				"specialValueType": "userName",
				"isIdentity": true,
				"name": "user",
				"displayName": "Username",
				"groupName": "Security",
				"valueType": "string",
				"defaultValue": null,
				"objectType": null,
				"categoryValues": null,
				"isRequired": false,
				"isArray": false
			  },
			  {
				"specialValueType": "password",
				"isIdentity": true,
				"name": "password",
				"displayName": "Password",
				"groupName": "Security",
				"valueType": "password",
				"defaultValue": null,
				"objectType": null,
				"categoryValues": null,
				"isRequired": false,
				"isArray": false
			  },
			  {
				"specialValueType": "connectionName",
				"isIdentity": true,
				"name": "connectionName",
				"displayName": "Connection Name",
				"groupName": "Source",
				"valueType": "string",
				"defaultValue": null,
				"objectType": null,
				"categoryValues": null,
				"isRequired": false,
				"isArray": false
			  }
			]
		  }
			},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"proposedapi": "node installTypings.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.39.0",
		"@types/azdata": "*",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.0",
		"typescript": "^4.3.5",
		"azdata-test": "^1.x"
	}
}
